<script id="EducationFilterView" type="text/html">
	<div class="EducationFilterView  rootFilterContainer">
		<div class="selectedItems education">
			<span class="add separateLine" style="display:none"></span>
			<span class="show autoCompleteContainer">
				<input type="text" class="autoComplete" data-type="education" placeholder="Enter a School"/>
				<label class="clear"><i></i></label>
			</span>
		</div>
		<div class="content education">
			<div class="autoCompleteList">
				<div class="center">loading...</div>
				{{#each educations}}
					<div class="educationRow"><span class="contentText" data-name="{{name}}">
						<span class="itemName">{{name}}</span>
						<span class="count">({{count}})</span></span>
					</div>
				{{/each}}
			</div>
		</div>
	</div>
</script>