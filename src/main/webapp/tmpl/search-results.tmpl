<script id="search-items" type="text/html">
  <table class="table">
    <thead>
      <tr>
        <th style="width:30px">ID</th>
        <th>Name</th>
        <th>Title</th>
      </tr>
    </thead>
    <tbody>
      {{#each items}}
      <tr>
          <td>{{id}}</td>
          <td>{{Name}}</td>
          <td>{{Title}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</script>